{
  "name": "latin",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clear": "expo start --clear",
    "android-clear": "expo start --clear --android",
    "ios-clear": "expo start --clear --ios",
    "build:android": "eas build --platform android --profile preview --local --no-wait",
    "install:clean": "rm -rf node_modules && npm install --legacy-peer-deps",
    "prebuild:cloud": "npm run install:clean && eas build --platform android --profile preview"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.23.0",
    "expo": "^52.0.38",
    "expo-dev-client": "~5.0.12",
    "expo-status-bar": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "18.2.0",
    "react-native": "^0.76.7",
    "react-native-web": "~0.19.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "resolutions": {
    "ws": "~7.4.6"
  }
}
